<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/navbar.css">
     <!-- CSS only -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
<!-- JavaScript Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@6.2.0/css/fontawesome.min.css" integrity="sha384-z4tVnCr80ZcL0iufVdGQSUzNvJsKjEtqYZjiQrrYKlpGow+btDHDfQWkFjoaz/Zr" crossorigin="anonymous">
    <title>Add vehicle Expenditure</title>
</head>
<body>


      <!--nav bar-->

      <div class="topnav">
        <a href="/">Home</a>
        <a href="/add-User">Add User</a>
        <a href="/add-Admin">Add Admin</a>
        <a class="active" href="/add-Vehicle-Expenditure">Add vehicle Expenditure</a>
        <a href="/fuelExpenditure">Fuel Expenditure</a>

      </div>

  <!--nav bar ends-->

    
    <header>
        <h1>Add Vehivcle Expenditure</h1>
    </header>


    <form action="/api/users" method="post" id="createVehicleExpenditure">

        <div class="container">
           <label for="Employee">Employee ID</label>
            <input type="text" placeholder="Enter a Employee ID" name="Employee" required>

            <label for="Name">Name</label>
            <input type="text" placeholder="Enter a Name" name="Name" required>


            <label for="Salary">Salary</label>
            <input type="text" placeholder="Enter Salary" name="Salary" required>

    
    
            <label for="Vehicles">Do you have vehicle</label>
            <input type="text" placeholder="Do you have vehicle" name="Vehicles" >

            <label for="Vehicle_type">vehicle type</label>
            <input type="text" placeholder="Enter your vehicle_type" name="Vehicle_type" >

            <label for="Vehicle_id">vehicle ID</label>
            <input type="text" placeholder="Enter your vehicle id" name="Vehicle_id" >

            <label for="Vehicle_no">Vehicle_no</label>
            <input type="text" placeholder="Enter your Vehicle_no" name="Vehicle_no" >

            <label for="Month">Month</label>
            <input type="text" placeholder="Enter your Month" name="Month" >

            <label for="Fuel_type">Fuel Type</label>
            <input type="text" placeholder="Enter a Fuel Type" name="Fuel_type" >

            <br>
            <label for="Meter_start">Meter Start(KM)</label>
            <br>
            <input type="text" placeholder="Enter your Meter Start(KM)" name="Meter_start">
    
            <br>
            <label for="Meter_end">Meter End(KM)</label>
            <br>
            <input type="text" placeholder="Enter your Meter End(KM)" name="Meter_end">
    
            <br>
            <label for="Traversal_perday_KM">Traversal Perday(KM)</label>
            <br>
            <input type="text" placeholder="Enter your Traversal Perday(KM)" name="Traversal_perday_KM">

            <br>
            <label for="Fuel_entered_perday">Fuel Entered Perday(Liter)</label>
            <br>
            <input type="text" placeholder="Enter your Fuel Entered Perday(Liter)" name="Fuel_entered_perday" >

            <br>
            <label for="Days_count">Days Count</label>
            <br>
            <input type="text" placeholder="Enter your Days Count" name="Days_count" >

            <br>
            <label for="Perday_cost">Fuel Cost Perday(Taka)</label>
            <br>
            <input type="text" placeholder="Enter your Fuel Cost Perday(Taka)" name="Perday_cost" >

            
            <br>
            <label for="Monthly_cost">Monthly Cost</label>
            <br>
            <input type="text" placeholder="Enter your Monthly Cost" name="Monthly_cost" >

            <br>
            <label for="Limit">Limit</label>
            <br>
            <input type="text" placeholder="Enter Limit" name="Limit" >

        
    
            <br>
            <br>
            <a href="/index"><button type="submit" class="button">Submit</button></a>

    
        </div>

    </form>

</body>
</html>

