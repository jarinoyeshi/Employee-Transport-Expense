                <input type="text" placeholder="Enter Exit Date" name="Exit_Date" required>

                <label for="Phone">Phone</label>
                <input type="text" placeholder="Enter your Phone" name="Phone" required>
        
                <a href="/index"><button type="submit" class="button">Submit</button></a>
    
        
