        <header>
            <h1>Add Driver Details </h1>
        </header>
    
    
        <form action="/api/driver" method="post" id="addAdmin">
    
            <div class="container">
     
               <label for="ID">Driver Id</label>
                <input type="text" placeholder="Enter a Admin ID" name="ID" >
    
                <label for="Full_Name">Full Name</label>
                <input type="text" placeholder="Enter your full name" name="Full_Name" required>
        
        
