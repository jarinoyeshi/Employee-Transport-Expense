            <a href="/addUser">Add User</a>
          </div>


    <!--nav bar ends-->


    <header>
        <h1>Update User</h1>
    </header>

    <form action="#" method="post">

      <div class="container">
        <label for="userid">User Id</label>
         <input type="text" placeholder="Enter a user ID" name="userid" value="<%= studentdata.userid %>">

         <label for="firstname">First Name</label>
         <input type="text" placeholder="Enter your first name" name="firstname" value="<%= studentdata.firstname %>">
 
         <label for="lastname">Last Name</label>
         <input type="text" placeholder="Enter your last name" name="lastname" value="<%= studentdata.lastname %>">
 
         <label for="phoneNumber">Phone Number</label>
         <input type="text" placeholder="Enter your phone number" name="phoneNumber" value="<%= studentdata.phoneNumber %>">
 
 
         <label for="email">Email</label>
         <input type="text" placeholder="Enter your email address" name="email" value="<%= studentdata.email %>">
 
         <a href=""><button type="submit" class="button">Submit</button></a>

  
      </div>

  </form>
  



    </div>
  
  

